    "import spacy\n",
    "from spacy import displacy\n",
    "nlp = spacy.load('en_core_web_md')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 389,
   "metadata": {},
   "outputs": [],
   "source": [
    "def fun_keyw(text,di,se,own):\n",
    "    doc = nlp(text)\n",
    "    se[str(own).lower()]=se.get(str(own).lower(),0)+1\n",
    "    for token in doc:\n",
    "        if token.is_alpha and (not token.is_stop) and token.pos_=='ADJ':\n",
    "            se[str(token).lower()]=se.get(str(token).lower(),0)+1\n",
    "            di[str(token).lower()]=di.get(str(token).lower(),0)+1\n",
    "    return di\n",
    "                \n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 421,
